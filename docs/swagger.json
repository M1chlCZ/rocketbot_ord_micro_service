    "host": "89.116.25.234:7500",
    "basePath": "/api",
    "paths": {
        "/getTransactions": {
            "post": {
                "description": "List transactions from BTC Core",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Transactions"
                ],
                "summary": "List transactions from BTC Core",
                "parameters": [
                    {
                        "description": "Info about device",
                        "name": "data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.TxRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "abandoned": {
                                        "type": "boolean"
                                    },
                                    "address": {
                                        "type": "string"
                                    },
                                    "amount": {
                                        "type": "number"
                                    },
                                    "bip125-replaceable": {
                                        "type": "string"
                                    },
                                    "blockhash": {
                                        "type": "string"
                                    },
                                    "blockheight": {
                                        "type": "integer"
                                    },
                                    "blockindex": {
                                        "type": "integer"
                                    },
                                    "blocktime": {
                                        "type": "integer"
                                    },
                                    "category": {
                                        "type": "string"
                                    },
                                    "confirmations": {
                                        "type": "integer"
                                    },
                                    "fee": {
                                        "type": "number"
                                    },
                                    "label": {
                                        "type": "string"
                                    },
                                    "parent_descs": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    "time": {
                                        "type": "integer"
                                    },
                                    "timereceived": {
                                        "type": "integer"
                                    },
                                    "trusted": {
                                        "type": "boolean"
                                    },
                                    "txid": {
                                        "type": "string"
                                    },
                                    "vout": {
                                        "type": "integer"
                                    },
                                    "walletconflicts": {
                                        "type": "array",
                                        "items": {}
                                    },
                                    "wtxid": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorHTTP"
                        }
                    },
                    "409": {
                        "description": "Conflict",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorHTTP"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorHTTP"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.ErrorHTTP": {
            "type": "object",
            "properties": {
                "errorMessage": {
                    "type": "string"
                },
                "hasError": {
                    "type": "boolean"
                },
                "status": {
                    "type": "string"
                }
            }
        },
        "models.TxRequest": {
            "type": "object",
            "properties": {
                "page": {
                    "type": "integer"
                },
                "pageSize": {
                    "type": "integer"
                }
            }
        }
    }
}