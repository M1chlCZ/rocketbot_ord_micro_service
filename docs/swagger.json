                                    },
                                    "fee": {
                                        "type": "number"
                                    },
                                    "label": {
                                        "type": "string"
                                    },
                                    "parent_descs": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    "time": {
                                        "type": "integer"
                                    },
                                    "timereceived": {
                                        "type": "integer"
                                    },
                                    "trusted": {
                                        "type": "boolean"
                                    },
                                    "txid": {
                                        "type": "string"
                                    },
                                    "vout": {
                                        "type": "integer"
                                    },
                                    "walletconflicts": {
                                        "type": "array",
                                        "items": {}
                                    },
                                    "wtxid": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorHTTP"
                        }
                    },
                    "409": {
                        "description": "Conflict",
                        "schema": {
